	@$(QMAKE) -o Makefile src.pro

dist: 
	@mkdir -p .tmp/calc && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/calc/ && ( cd `dirname .tmp/calc` && $(TAR) calc.tar calc && $(GZIP) calc.tar ) && $(MOVE) `dirname .tmp/calc`/calc.tar.gz . && $(DEL_FILE) -r .tmp/calc

